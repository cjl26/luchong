.page {
  font-size: 95%;
  animation: fadeInUp 0.2s ease-out;
}
scroll-view{
  position: absolute;
  left: 0;
  top: 0;
  bottom: 120rpx;
  width: 100vw;
}
.details-header {
  padding: 1em 0.8em 0 0.8em;
}

.details-title {
  padding: 0.5em 0 0.2em 0;
  font-size: 1.2em;
}

.details-header {
  background-color: #fff;
}
.details-status{
  display: flex;
  justify-content:space-between;
  align-items: center;
}
.status-info {
  display: flex;
  align-items: center;
  font-size: 90%;
  color: #999;
}

.status-info view {
  margin: 0 1em 0 0;
}

.author-info{
  display: flex;
  align-items: center;
}

.author-info image {
  width: 2em;
  height: 2em;
  border-radius: 100%;
  margin-right: 0.5em;
}

.author-info text {
  max-width: 6em;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.follow-btn{
  min-width: 3em;
  padding: 4rpx 24rpx;
  border-radius: 0.2em;
  border: 1px solid #ffa000;
  text-align: center;
  color: #ffa000;
}
.follow-btn.followed{
  border: 1px solid #ccc;
  color: #ccc;
}

.details-body {
  padding: 0.8em;
  line-height: 1.6em;
  background-color: #fff;
}

.details-body text {
  display: block;
  margin: 0 0 0.3em 0;
}

.details-body image {
  width: 100%;
  margin: 0 0 0.5em 0;
}
.like-list{
  padding: 0.5em 0.8em;
  margin: 0.5em 0;
  background-color: #fff;
}
.like-list text{
  color: #999;
}
.favorer-list{
  display: flex;
  flex-wrap: nowrap;
  overflow: auto;
  padding: 0.8em 0 0.5em 0;
}
.favorer-list .favorer{
  margin: 0 0.2em;
  flex-shrink: 0;
}
.favorer-list .favorer image{
  width: 2.4em;
  height: 2.4em;
  border-radius: 100%;
}

.comment-item{
  padding: 0.5em 0.8em;
  border-bottom: 1px solid #eee;
  background-color: #fff;
}
.comment-header{
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #999;
}

.header-left{
  display: flex;
  align-items: center;
}
.header-right{
  font-size: 90%;
}
.header-left image{
  width: 1.8em;
  height: 1.8em;
  border-radius: 100%;
}
.header-left .nickname{
  margin: 0 0.5em;
}
.header-left .small-time{
  font-size: 80%;
}
.comment-box{
  background-color: #fff;
}
.comment-title{
  color:#999;
  padding: 0.5em 0.8em;
}
.comment-body{
  line-height: 1.6em;
  padding: 0.2em 0 0.2em 2.3em;
  font-size: 95%;
}
.comment-reply{
  padding: 0 0 0 2em;
}
.reply-list{
  background-color: #f2f2f2;
  padding: 0.3em 0.5em ;
  line-height: 1.6em;
  border-radius: 0.2em;
  font-size: 90%;
}
.reply-item{
  margin: 0.2em 0 0 0;
}

.details-toolbar {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 112rpx;
  padding: 0 0.8em;
  background-color: rgba(250, 250, 250, 0.92);
  border-top: 1px solid #eee;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 90%;
  color: #999;
}

.details-toolbar .comm-btn {
  line-height: 2.2em;
  width: 70%;
  padding: 0 1em;
  border-radius: 1.1em;
  background-color: #fff;
  border: 1px solid #ddd;
  box-sizing: border-box;
}

.btn-group {
  width: 30%;
  display: flex;
}

.details-toolbar .like-btn, .details-toolbar .favor-btn,
.details-toolbar .share-btn {
  position: relative;
  width: 1.8em;
  height: 1.8em;
  background-position:center;
  background-repeat: no-repeat;
  background-size: cover;
  margin-left:1em;
}
.details-toolbar .like-btn{
  background-image: url('data:image/png;base64,');
  background-size: 95%;
}
.details-toolbar .favor-btn{
  background-image: url('data:image/png;base64,');
}
.details-toolbar .share-btn{
  background-image: url('data:image/png;base64,');
}
.details-toolbar .like-btn.active, .details-toolbar .favor-btn.active,
.details-toolbar .share-btn.active{
  background-image: none;
}

.details-toolbar .like-btn.active::after{
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: url('data:image/png;base64,') no-repeat 50% 50% /95%;
  animation: zoomBigIn 0.3s;
}


.details-toolbar .favor-btn.active::after{
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: url('data:image/png;base64,') no-repeat 50% 50% /cover;
  animation: zoomBigIn 0.3s;
}


@keyframes zoomBigIn{
  0%{
    opacity: 0;
    transform: scale3d(0.2,0.2,1);
  }
  100%{
    opacity: 1;
    transform: none;
  }
}


